{
	"name": "mediawiki/maps",
	"type": "mediawiki-extension",
	"description": "Adds various mapping features to MediaWiki",
	"keywords": [
		"MediaWiki",
		"Semantic MediaWiki",
		"Maps",
		"Google Maps",
		"OpenLayers",
		"OSM",
		"OpenStreetMap"
	],
	"homepage": "https://github.com/JeroenDeDauw/Maps",
	"license": "GPL-2.0+",
	"authors": [
		{
			"name": "Jeroen De Dauw",
			"email": "jeroendedauw@gmail.com",
			"homepage": "http://jeroendedauw.com",
			"role": "Creator and lead developer"
		}
	],
	"support": {
		"email": "semediawiki-user@lists.sourceforge.net",
		"irc": "irc://irc.freenode.net/mediawiki",
		"source": "https://github.com/JeroenDeDauw/Maps"
	},
	"require": {
		"php": ">=5.3.2",
		"composer/installers": "1.*,>=1.0.1",
		"mediawiki/validator": "1.*,>=1.0.1",
		"data-values/geo": "0.*,>=0.1.1"
	},
	"autoload": {
		"files" : [
			"Maps.php"
		],
		"classmap": [
			"src/",
			"includes/",
			"Maps.hooks.php",
			"tests/phpunit/elements/",
			"tests/phpunit/parserhooks/ParserHookTest.php"
		]
	},
	"extra": {
		"branch-alias": {
			"dev-master": "3.x-dev"
		}
	}
}
